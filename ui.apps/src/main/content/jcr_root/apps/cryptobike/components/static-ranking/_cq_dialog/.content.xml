<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Static Ranking"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured"
               sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <content-tab jcr:primaryType="nt:unstructured"
                            jcr:title="Content"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <title cq-msm-lockable="./title"
                                  fieldLabel="Title"
                                  jcr:primaryType="nt:unstructured"
                                  name="./title"
                                  sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <about jcr:primaryType="nt:unstructured"
                                  sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                  editorType="text"
                                  fieldLabel="Description"
                                  name="./about"
                                  useFixedInlineToolbar="{Boolean}true">
                              <rtePlugins jcr:primaryType="nt:unstructured">
                                 <format jcr:primaryType="nt:unstructured" features="*" />
                              </rtePlugins>
                              <uiSettings jcr:primaryType="nt:unstructured">
                                 <cui jcr:primaryType="nt:unstructured">
                                    <inline jcr:primaryType="nt:unstructured" toolbar="[format#bold,format#italic,format#underline]"></inline>
                                 </cui> 
                              </uiSettings>
                           </about>
                           <buttonTitle cq-msm-lockable="./buttonTitle"
                                        fieldLabel="Button Title"
                                        jcr:primaryType="nt:unstructured"
                                        name="./buttonTitle"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <buttonUrl cq-msm-lockable="./buttonUrl"
                                      fieldLabel="Button URL"
                                      jcr:primaryType="nt:unstructured"
                                      name="./buttonUrl"
                                      sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
                        </items>
                     </column>
                  </items>
               </content-tab>
               <other-tab jcr:primaryType="nt:unstructured"
                          jcr:title="Other"
                          sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <missingCountryMessage cq-msm-lockable="./missingCountryMessage"
                                                  fieldLabel="Missing Country Message"
                                                  jcr:primaryType="nt:unstructured"
                                                  name="./missingCountryMessage"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </other-tab>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>
